{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "http": {
      "routePrefix": "user"
    }
  },
  "functionTimeout": "00:06:00",
  "functions": [ "get", "new", "patch", "delete" ],
  "watchDirectories": [ "models", "config" ]
}
